{
  "name": "@nself/plugin-file-processing",
  "version": "1.0.0",
  "description": "File processing plugin for nself with thumbnails, optimization, and virus scanning",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": {
    "nself-file-processing": "./dist/cli.js"
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc --watch",
    "start": "node dist/server.js",
    "dev": "tsx watch src/server.ts",
    "worker": "node dist/worker.js",
    "clean": "rm -rf dist",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@nself/plugin-utils": "file:../../../shared",
    "fastify": "^4.24.0",
    "@fastify/cors": "^8.4.0",
    "@fastify/multipart": "^8.0.0",
    "dotenv": "^16.3.1",
    "commander": "^11.1.0",
    "sharp": "^0.33.1",
    "bullmq": "^5.0.0",
    "ioredis": "^5.3.2",
    "@aws-sdk/client-s3": "^3.485.0",
    "@aws-sdk/lib-storage": "^3.485.0",
    "@azure/storage-blob": "^12.17.0",
    "@google-cloud/storage": "^7.7.0",
    "fluent-ffmpeg": "^2.1.2",
    "clamscan": "^2.1.2",
    "exifreader": "^4.18.0",
    "file-type": "^19.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/fluent-ffmpeg": "^2.1.24",
    "@types/pg": "^8.11.10",
    "typescript": "^5.3.0",
    "tsx": "^4.6.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "license": "MIT"
}
