        {"name": "prs", "description": "List pull requests"}
      ],
      "apiEndpoints": [
        {"method": "POST", "path": "/webhook", "description": "GitHub webhook receiver"},
        {"method": "POST", "path": "/api/sync", "description": "Trigger data sync"},
        {"method": "GET", "path": "/api/status", "description": "Get sync status"},
        {"method": "GET", "path": "/api/repositories", "description": "List repositories"},
        {"method": "GET", "path": "/api/issues", "description": "List issues"},
        {"method": "GET", "path": "/api/pull-requests", "description": "List pull requests"},
