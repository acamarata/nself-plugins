{
  "$schema": "./registry-schema.json",
  "version": "1.0.0",
  "lastUpdated": "2026-01-24T12:00:00Z",
  "plugins": {
    "stripe": {
      "name": "stripe",
      "version": "1.0.0",
      "description": "Stripe billing data sync with webhook handling",
      "author": "nself",
      "license": "Source-Available",
      "homepage": "https://github.com/acamarata/nself-plugins/tree/main/plugins/stripe",
      "repository": "https://github.com/acamarata/nself-plugins",
      "path": "plugins/stripe",
      "minNselfVersion": "0.4.8",
      "category": "billing",
      "tags": ["payments", "billing", "subscriptions", "stripe"],
      "implementation": {
        "language": "typescript",
        "runtime": "node",
        "minNodeVersion": "18.0.0",
        "entryPoint": "ts/dist/index.js",
        "cli": "ts/dist/cli.js",
        "cliName": "nself-stripe",
        "defaultPort": 3001,
        "packageManager": "npm",
        "framework": "fastify"
      },
      "tables": [
        "stripe_customers",
        "stripe_products",
        "stripe_prices",
        "stripe_subscriptions",
        "stripe_invoices",
        "stripe_payment_intents",
        "stripe_payment_methods",
        "stripe_webhook_events"
      ],
      "views": [
        "stripe_active_subscriptions",
        "stripe_mrr",
        "stripe_failed_payments"
      ],
      "webhooks": [
        "customer.created",
        "customer.updated",
        "customer.deleted",
        "product.created",
        "product.updated",
        "product.deleted",
        "price.created",
        "price.updated",
        "price.deleted",
        "subscription.created",
        "subscription.updated",
        "subscription.deleted",
        "invoice.created",
        "invoice.updated",
        "invoice.paid",
        "invoice.payment_failed",
        "invoice.finalized",
        "payment_intent.created",
        "payment_intent.succeeded",
        "payment_intent.payment_failed",
        "payment_intent.canceled",
        "payment_method.attached",
        "payment_method.detached",
        "payment_method.updated"
      ],
      "cliCommands": [
        {"name": "sync", "description": "Sync all Stripe data to database"},
        {"name": "server", "description": "Start the webhook server"},
        {"name": "init", "description": "Initialize database schema"},
        {"name": "status", "description": "Show sync status and statistics"},
        {"name": "customers", "description": "List customers"},
        {"name": "subscriptions", "description": "List subscriptions"},
        {"name": "invoices", "description": "List invoices"},
        {"name": "products", "description": "List products"},
        {"name": "prices", "description": "List prices"}
      ],
      "apiEndpoints": [
        {"method": "POST", "path": "/webhook", "description": "Stripe webhook receiver"},
        {"method": "POST", "path": "/api/sync", "description": "Trigger data sync"},
        {"method": "GET", "path": "/api/status", "description": "Get sync status"},
        {"method": "GET", "path": "/api/customers", "description": "List customers"},
        {"method": "GET", "path": "/api/subscriptions", "description": "List subscriptions"},
        {"method": "GET", "path": "/api/invoices", "description": "List invoices"},
        {"method": "GET", "path": "/api/products", "description": "List products"},
        {"method": "GET", "path": "/api/mrr", "description": "Get MRR summary"}
      ],
      "envVars": [
        {"name": "STRIPE_API_KEY", "required": true, "description": "Stripe API secret key"},
        {"name": "STRIPE_WEBHOOK_SECRET", "required": false, "description": "Webhook signing secret"},
        {"name": "DATABASE_URL", "required": true, "description": "PostgreSQL connection string"},
        {"name": "PORT", "required": false, "default": "3001", "description": "Server port"},
        {"name": "HOST", "required": false, "default": "0.0.0.0", "description": "Server host"}
      ],
      "dependencies": ["pg", "stripe", "fastify", "commander"],
      "checksums": {"sha256": ""}
    },
