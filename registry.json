        {"method": "GET", "path": "/api/releases", "description": "List releases"}
      ],
      "envVars": [
        {"name": "GITHUB_TOKEN", "required": true, "description": "GitHub personal access token"},
        {"name": "GITHUB_WEBHOOK_SECRET", "required": false, "description": "Webhook signing secret"},
        {"name": "GITHUB_ORG", "required": false, "description": "GitHub organization to sync"},
        {"name": "GITHUB_REPOS", "required": false, "description": "Comma-separated list of repos"},
        {"name": "DATABASE_URL", "required": true, "description": "PostgreSQL connection string"},
        {"name": "PORT", "required": false, "default": "3002", "description": "Server port"},
        {"name": "HOST", "required": false, "default": "0.0.0.0", "description": "Server host"}
      ],
      "dependencies": ["pg", "@octokit/rest", "@octokit/webhooks", "fastify", "commander"],
      "checksums": {"sha256": ""}
    },
